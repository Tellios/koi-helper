{
  "name": "tellios-koi-helper",
  "version": "0.0.1",
  "description": "Application for helping you with saving data about your koi fish",
  "main": "main.js",
  "author": "Sonny Johannesson",
  "license": "MIT",
  "owner": "Tellios",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tellios/koi-helper.git"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf dist",
    "build": "npm run clean && electron-webpack",
    "build:watch": "npm run clean && tsc --watch",
    "pack": "electron-builder --dir",
    "dist:linux": "electron-builder --linux",
    "dist:windows": "electron-builder --windows",
    "publish": "electron-builder --publish always",
    "prettier": "prettier --config prettier.config.js --write src/**/*",
    "prettier-ci": "prettier --config prettier.config.js --list-different src/**/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-devtool": "overmind-devtools",
    "start": "electron-webpack dev"
  },
  "build": {
    "appId": "com.tellios.koi.helper",
    "productName": "Koi Helper",
    "artifactName": "KoiHelper-${version}.${ext}",
    "win": {
      "publish": [
        "github"
      ]
    },
    "publish": {
      "provider": "github",
      "owner": "Tellios"
    },
    "files": [
      {
        "from": "src",
        "to": "dist"
      }
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.10",
    "@types/react-dom": "^16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-toastify": "^4.0.2",
    "@types/recharts": "^1.8.3",
    "@types/sharp": "^0.23.0",
    "@types/sqlite3": "^3.1.5",
    "@types/uuid": "^3.4.5",
    "aws-sdk": "^2.590.0",
    "circular-dependency-plugin": "^5.2.0",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "overmind-devtools": "^21.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "date-fns": "^2.8.1",
    "env-paths": "^2.2.0",
    "formik": "^1.5.8",
    "formik-material-ui": "0.0.22",
    "fs-extra": "^8.1.0",
    "i18n-ts": "^1.0.5",
    "inversify": "^5.0.1",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "overmind": "^20.0.0",
    "overmind-react": "^21.0.0",
    "p-limit": "^2.2.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-intersection-observer": "^8.25.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.4.0",
    "recharts": "^2.0.0-beta.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.23.4",
    "source-map-support": "^0.5.16",
    "sqlite3": "^4.1.1",
    "typeface-roboto": "0.0.75",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "yup": "^0.27.0"
  }
}
