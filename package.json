{
  "name": "tellios-fish-manager",
  "version": "0.0.0",
  "description": "Application for helping you with saving data about your fish pets",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "clean": "rimraf dist",
    "build": "npm run clean && electron-webpack",
    "build:watch": "npm run clean && tsc --watch",
    "dist": "npm run build && electron-builder",
    "prettier": "prettier --config prettier.config.js --write src/**/*",
    "prettier-ci": "prettier --config prettier.config.js --list-different src/**/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"electron-webpack dev\" \"overmind-devtools\""
  },
  "author": "Sonny Johannesson",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/sqlite3": "^3.1.5",
    "concurrently": "^4.1.0",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.2.0",
    "overmind-devtools": "^19.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0"
  },
  "dependencies": {
    "antd": "^3.19.3",
    "fs-extra": "^8.0.1",
    "inversify": "^5.0.1",
    "overmind": "^18.0.1",
    "overmind-react": "^19.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.9"
  }
}
